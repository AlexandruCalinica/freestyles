{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2020",                                  
    "module": "ES2020",
    "moduleResolution": "Node",
    "declaration": true,                                
    "esModuleInterop": true,                             
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                     
    "skipLibCheck": true                                 
  }
}
